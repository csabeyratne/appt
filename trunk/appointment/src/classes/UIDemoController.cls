      public List<SelectOption> getFieldNames()
      {
                 Map<String, Schema.SObjectField> m = myObject.getSObjectType().getDescribe().fields.getMap();
                 List<String> fieldNames = new List<String>();
                 fieldNames.addAll(m.keySet());
                 List<SelectOption> fieldNamesSO = new List<SelectOption>();
                 for(String fn : fieldNames)
                     fieldNamesSO.add(new SelectOption(fn,fn));
                 return fieldNamesSO;
      }
      
