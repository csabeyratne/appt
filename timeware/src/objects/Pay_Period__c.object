        <lookupPhoneDialogsAdditionalFields>Pay_Period_Start__c</lookupPhoneDialogsAdditionalFields>
        <lookupPhoneDialogsAdditionalFields>Pay_Period_End__c</lookupPhoneDialogsAdditionalFields>
