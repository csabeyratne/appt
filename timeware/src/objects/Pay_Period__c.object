        <searchFilterFields>Pay_Period_Start__c</searchFilterFields>
        <searchFilterFields>Pay_Period_End__c</searchFilterFields>
    </searchLayouts>
    <sharingModel>ControlledByParent</sharingModel>
</CustomObject>
